{
    "configurations": [
      {
        "name": "ESP32",
        "includePath": [
          "${workspaceFolder}/**",
          "${env:IDF_PATH}/components/**",
          "${env:IDF_PATH}/components/esp32-camera/**",
          "${env:IDF_PATH}/components/driver/include/**",
          "${env:IDF_PATH}/components/freertos/include/**",
          "${env:IDF_PATH}/components/esp_wifi/include/**",
          "${env:IDF_PATH}/components/esp_common/include/**",
          "${env:IDF_PATH}/components/esp_system/include/**",
          "${env:IDF_PATH}/components/nvs_flash/include/**",
          "${env:IDF_PATH}/components/spi_flash/include/**",
          "${workspaceFolder}/libraries/Firebase_ESP_Client/src/**"
        ],
        "compilerPath": "${env:IDF_TOOLS_PATH}/tools/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "intelliSenseMode": "gcc-x64",
        "defines": [
          "ESP_PLATFORM",
          "ARDUINO=10813",
          "ESP32"
        ]
      }
    ],
    "version": 4
  }
  
